{
  "name": "bisheng-plugin-ice",
  "version": "0.1.9",
  "description": "To support ant.design-like websites.",
  "scripts": {
    "lint": "eslint ./src",
    "eslint-fix": "eslint --fix ./src",
    "babel": "babel ./src --out-dir ./lib",
    "build": "npm run babel && cp ./src/template.html ./lib",
    "pub": "npm run babel && cp ./src/template.html ./lib && npm publish && rm -rf lib",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noyobo/bisheng-plugin-ice.git"
  },
  "keywords": [
    "bisheng",
    "plugin"
  ],
  "author": "noyobo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noyobo/bisheng-plugin-ice/issues"
  },
  "homepage": "https://github.com/noyobo/bisheng-plugin-ice#readme",
  "dependencies": {
    "antd": "^1.5.0",
    "babel-core": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "classnames": "^2.2.5",
    "jsonml.js": "^0.1.0",
    "nunjucks": "^2.4.2",
    "react-sublime-video": "^0.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.13.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-router": "^2.0.0",
    "jsonml-to-react-component": "^0.2.0"
  }
}
